		"modmenu": [
			"com.auroali.sanguinisluxuria.compat.ModMenuCompat"
		],
		"appleskin": [
			"com.auroali.sanguinisluxuria.compat.AppleskinCompat"
		],
		"emi": [
			"com.auroali.sanguinisluxuria.compat.emi.EmiCompat"
