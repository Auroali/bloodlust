{
	"schemaVersion": 1,
	"id": "sanguinisluxuria",
	"version": "${version}",
	"name": "Sanguinis Luxuria",
	"description": "A mod about becoming a vampire",
	"authors": [
		"Auroali",
		"EastCoastJack"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/sanguinis-luxuria",
		"sources": "https://github.com/Auroali/sanguinis-luxuria",
		"issues": "https://github.com/Auroali/sanguinis-luxuria/issues"
	},
	"license": "MIT",
	"icon": "assets/sanguinisluxuria/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.auroali.sanguinisluxuria.Bloodlust"
		],
		"client": [
			"com.auroali.sanguinisluxuria.BloodlustClient"
		],
		"fabric-datagen": [
			"com.auroali.sanguinisluxuria.BloodlustDataGenerator"
		],
